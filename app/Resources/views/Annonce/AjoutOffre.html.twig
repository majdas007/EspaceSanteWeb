{% extends "default/home.html.twig" %}
{% block content %}
    <br><br>
    <center>
    <div style="width: 400px" >

        {{ form_start(form) }}
        <fieldset>
            <legend>Ajout√© un Offre d'emploi</legend>

        <div class="form-group">
            {{ form_widget(form.titreAnnonce, { 'attr': {'class': 'form-control', 'placeholder': 'titre de l\'offre'} }) }}
            {{ form_errors(form.titreAnnonce) }}
        </div>
            <br><br>
        <div class="form-group">
            {{ form_widget(form.dateAnnonce) }}
            {{ form_errors(form.dateAnnonce) }}
        </div>
        <br><br>
        <div class="form-group">
            {{ form_widget(form.descAnnonce, { 'attr': {'class': 'form-control', 'placeholder': 'Description'} }) }}
            {{ form_errors(form.descAnnonce) }}
        </div>
            <br><br>
        <div class="form-group">
            {{ form_widget(form.addrAnnonce, { 'attr': {'class': 'form-control', 'placeholder': 'adresse'} }) }}
            {{ form_errors(form.addrAnnonce) }}
        </div>

        <br><br>
        <div class="form-group">
            {{ form_widget(form.telAnnonce, { 'attr': {'class': 'form-control', 'placeholder': 'telephone'} }) }}
            {{ form_errors(form.telAnnonce) }}
        </div>
        <br><br>
        <div class="form-group">
            {{ form_widget(form.file) }}
            {{ form_errors(form.file) }}
        </div>
            <div>
                <input type="submit" value="Ajouter"   href="{{ asset('css/button')}}"/>

            </div>
        <br><br>
        </fieldset>
        {{ form_end(form) }}
    </div>

    </center>
    <br><br>
{% endblock content %}